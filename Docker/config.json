{
  "name": "NetDaemon",
  "version": "dev",
  "slug": "netdaemon",
  "description": "Application daemon written in .NET core 3.1",
  "url": "http://netdaemon.xyz",
  "startup": "application",
  "image": "netdaemon/netdaemon",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7"
  ],
  "boot": "auto",
  "map": [
    "config:rw"
  ],
  "hassio_api": true,
  "hassio_role": "default",
  "homeassistant_api": true,
  "host_network": false,
  "options": {
    "log_level": "info"
  },
  "schema": {
    "log_level": "match(^(trace|debug|info|warning|error)$)"
  }
}